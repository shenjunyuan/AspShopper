@model vmAccountProfile
<div>
    <table class="account-profile-table">
        <tr>
            <td class="account-profile-box-detail-header text-bold">
                @Html.DisplayNameFor(model => model.AccountNo)
            </td>
            <td>
                @Html.DisplayFor(model => model.AccountNo)
            </td>
        </tr>
        <tr>
            <td class="account-profile-box-detail-header text-bold">
                @Html.DisplayNameFor(model => model.AccountName)
            </td>
            <td>
                @Html.DisplayFor(model => model.AccountName)
            </td>
        </tr>
        <tr>
            <td class="account-profile-box-detail-header text-bold">
                @Html.DisplayNameFor(model => model.ContactEmail)
            </td>
            <td>
                @Html.DisplayFor(model => model.ContactEmail)
            </td>
        </tr>
    </table>
</div>