
@{
    ViewBag.Title = "ValidateForgetCode";
    Layout = "~/Views/Shared/_LayoutLite.cshtml";
}

<div class="form-screen">
    <div class="form-horizontal">
        <div class="message-box">
            <div class="card">
                <div class="card-header text-center">
                    <h3>@SessionService.AppName - 忘記密碼重設結果</h3>
                </div>
                <div class="card-body">
                    <p class="card-text">@ViewBag.MessageText</p>
                    @Html.ActionLink("會員登入", "Login", "Account", null, new { @class = "btn btn-primary" })
                </div>
            </div>
        </div>
    </div>
</div>